<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#">
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
    <!ENTITY iso5259 "http://pebblous.ai/ontology/iso5259-2#">
]>

<rdf:RDF xmlns="http://pebblous.ai/ontology/iso5259-2#"
     xml:base="http://pebblous.ai/ontology/iso5259-2"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:iso5259="http://pebblous.ai/ontology/iso5259-2#">

    <!-- Ontology Metadata -->
    <owl:Ontology rdf:about="http://pebblous.ai/ontology/iso5259-2">
        <rdfs:label xml:lang="en">ISO/IEC 5259-2 Data Quality Ontology</rdfs:label>
        <rdfs:label xml:lang="ko">ISO/IEC 5259-2 데이터 품질 온톨로지</rdfs:label>
        <rdfs:comment xml:lang="en">An ontology representing data quality characteristics and measures for analytics and machine learning based on ISO/IEC 5259-2:2024</rdfs:comment>
        <rdfs:comment xml:lang="ko">ISO/IEC 5259-2:2024 표준을 기반으로 한 분석 및 머신러닝을 위한 데이터 품질 특성과 측정 지표를 나타내는 온톨로지</rdfs:comment>
        <owl:versionInfo>1.0.0</owl:versionInfo>
        <owl:versionIRI rdf:resource="http://pebblous.ai/ontology/iso5259-2/1.0.0"/>
    </owl:Ontology>

    <!--
    =====================================================================
    Classes - 클래스 정의
    =====================================================================
    -->

    <!-- Top-level Classes -->

    <owl:Class rdf:about="&iso5259;DataQualityModel">
        <rdfs:label xml:lang="en">Data Quality Model</rdfs:label>
        <rdfs:label xml:lang="ko">데이터 품질 모델</rdfs:label>
        <rdfs:comment xml:lang="en">A framework for specifying data quality requirements and evaluating data quality</rdfs:comment>
        <rdfs:comment xml:lang="ko">데이터 품질 요구사항을 명시하고 평가하기 위한 프레임워크</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="&iso5259;DataQualityCharacteristic">
        <rdfs:label xml:lang="en">Data Quality Characteristic</rdfs:label>
        <rdfs:label xml:lang="ko">데이터 품질 특성</rdfs:label>
        <rdfs:comment xml:lang="en">A category of data quality attributes that bear on data quality</rdfs:comment>
        <rdfs:comment xml:lang="ko">데이터 품질에 영향을 미치는 데이터 품질 속성의 범주</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="&iso5259;DataQualityMeasure">
        <rdfs:label xml:lang="en">Data Quality Measure</rdfs:label>
        <rdfs:label xml:lang="ko">데이터 품질 측정 지표</rdfs:label>
        <rdfs:comment xml:lang="en">A variable to which a value is assigned as the result of measurement</rdfs:comment>
        <rdfs:comment xml:lang="ko">측정 결과로 값이 할당되는 변수</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="&iso5259;MeasurementFunction">
        <rdfs:label xml:lang="en">Measurement Function</rdfs:label>
        <rdfs:label xml:lang="ko">측정 함수</rdfs:label>
        <rdfs:comment xml:lang="en">Algorithm or calculation performed to combine one or more quality measure elements</rdfs:comment>
        <rdfs:comment xml:lang="ko">하나 이상의 품질 측정 요소를 결합하기 위해 수행되는 알고리즘 또는 계산</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="&iso5259;TargetData">
        <rdfs:label xml:lang="en">Target Data</rdfs:label>
        <rdfs:label xml:lang="ko">대상 데이터</rdfs:label>
        <rdfs:comment xml:lang="en">Data used in an analytics or ML task whose quality is measured</rdfs:comment>
        <rdfs:comment xml:lang="ko">품질이 측정되는 분석 또는 ML 작업에 사용되는 데이터</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="&iso5259;DataItem">
        <rdfs:subClassOf rdf:resource="&iso5259;TargetData"/>
        <rdfs:label xml:lang="en">Data Item</rdfs:label>
        <rdfs:label xml:lang="ko">데이터 항목</rdfs:label>
        <rdfs:comment xml:lang="en">Individual data element consisting of item name, data value, and data type</rdfs:comment>
        <rdfs:comment xml:lang="ko">항목 이름, 데이터 값, 데이터 타입으로 구성된 개별 데이터 요소</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="&iso5259;DataSet">
        <rdfs:subClassOf rdf:resource="&iso5259;TargetData"/>
        <rdfs:label xml:lang="en">Dataset</rdfs:label>
        <rdfs:label xml:lang="ko">데이터셋</rdfs:label>
        <rdfs:comment xml:lang="en">Collection of data items, records, or frames</rdfs:comment>
        <rdfs:comment xml:lang="ko">데이터 항목, 레코드 또는 프레임의 모음</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="&iso5259;DataQualityRequirement">
        <rdfs:label xml:lang="en">Data Quality Requirement</rdfs:label>
        <rdfs:label xml:lang="ko">데이터 품질 요구사항</rdfs:label>
        <rdfs:comment xml:lang="en">Properties or attributes of the data along with acceptance criteria</rdfs:comment>
        <rdfs:comment xml:lang="ko">수락 기준과 함께 데이터의 속성 또는 특성</rdfs:comment>
    </owl:Class>

    <!-- Data Quality Characteristic Categories -->

    <owl:Class rdf:about="&iso5259;InherentCharacteristic">
        <rdfs:subClassOf rdf:resource="&iso5259;DataQualityCharacteristic"/>
        <rdfs:label xml:lang="en">Inherent Data Quality Characteristic</rdfs:label>
        <rdfs:label xml:lang="ko">고유 데이터 품질 특성</rdfs:label>
        <rdfs:comment xml:lang="en">Data quality characteristic independent of system</rdfs:comment>
        <rdfs:comment xml:lang="ko">시스템과 무관한 데이터 품질 특성</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="&iso5259;SystemDependentCharacteristic">
        <rdfs:subClassOf rdf:resource="&iso5259;DataQualityCharacteristic"/>
        <rdfs:label xml:lang="en">System-Dependent Data Quality Characteristic</rdfs:label>
        <rdfs:label xml:lang="ko">시스템 의존 데이터 품질 특성</rdfs:label>
        <rdfs:comment xml:lang="en">Data quality characteristic dependent on system</rdfs:comment>
        <rdfs:comment xml:lang="ko">시스템에 의존하는 데이터 품질 특성</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="&iso5259;HybridCharacteristic">
        <rdfs:subClassOf rdf:resource="&iso5259;DataQualityCharacteristic"/>
        <rdfs:label xml:lang="en">Inherent and System-Dependent Characteristic</rdfs:label>
        <rdfs:label xml:lang="ko">고유 및 시스템 의존 특성</rdfs:label>
        <rdfs:comment xml:lang="en">Data quality characteristic that is both inherent and system-dependent</rdfs:comment>
        <rdfs:comment xml:lang="ko">고유 및 시스템 의존적 특성을 모두 가진 데이터 품질 특성</rdfs:comment>
    </owl:Class>

    <!-- Inherent Characteristics (Section 6.2) -->

    <owl:Class rdf:about="&iso5259;Accuracy">
        <rdfs:subClassOf rdf:resource="&iso5259;InherentCharacteristic"/>
        <rdfs:label xml:lang="en">Accuracy</rdfs:label>
        <rdfs:label xml:lang="ko">정확성</rdfs:label>
        <rdfs:comment xml:lang="en">Degree to which data items have correct data values or labels</rdfs:comment>
        <rdfs:comment xml:lang="ko">데이터 항목이 올바른 데이터 값 또는 레이블을 가지는 정도</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="&iso5259;Completeness">
        <rdfs:subClassOf rdf:resource="&iso5259;InherentCharacteristic"/>
        <rdfs:label xml:lang="en">Completeness</rdfs:label>
        <rdfs:label xml:lang="ko">완전성</rdfs:label>
        <rdfs:comment xml:lang="en">Degree to which data have values for all expected attributes</rdfs:comment>
        <rdfs:comment xml:lang="ko">모든 예상 속성에 대한 값을 데이터가 가지는 정도</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="&iso5259;Consistency">
        <rdfs:subClassOf rdf:resource="&iso5259;InherentCharacteristic"/>
        <rdfs:label xml:lang="en">Consistency</rdfs:label>
        <rdfs:label xml:lang="ko">일관성</rdfs:label>
        <rdfs:comment xml:lang="en">Degree to which data are coherent and free of contradictions</rdfs:comment>
        <rdfs:comment xml:lang="ko">데이터가 일관성 있고 모순이 없는 정도</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="&iso5259;Credibility">
        <rdfs:subClassOf rdf:resource="&iso5259;InherentCharacteristic"/>
        <rdfs:label xml:lang="en">Credibility</rdfs:label>
        <rdfs:label xml:lang="ko">신뢰성</rdfs:label>
        <rdfs:comment xml:lang="en">Degree to which data are regarded as believable by users</rdfs:comment>
        <rdfs:comment xml:lang="ko">사용자가 데이터를 믿을 수 있는 것으로 간주하는 정도</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="&iso5259;Currentness">
        <rdfs:subClassOf rdf:resource="&iso5259;InherentCharacteristic"/>
        <rdfs:label xml:lang="en">Currentness</rdfs:label>
        <rdfs:label xml:lang="ko">현행성</rdfs:label>
        <rdfs:comment xml:lang="en">Degree to which data are of the right age for the task</rdfs:comment>
        <rdfs:comment xml:lang="ko">작업에 적합한 시기의 데이터인 정도</rdfs:comment>
    </owl:Class>

    <!-- Hybrid Characteristics (Section 6.3) -->

    <owl:Class rdf:about="&iso5259;Accessibility">
        <rdfs:subClassOf rdf:resource="&iso5259;HybridCharacteristic"/>
        <rdfs:label xml:lang="en">Accessibility</rdfs:label>
        <rdfs:label xml:lang="ko">접근성</rdfs:label>
        <rdfs:comment xml:lang="en">Degree to which data can be accessed in a specific context</rdfs:comment>
        <rdfs:comment xml:lang="ko">특정 맥락에서 데이터에 접근할 수 있는 정도</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="&iso5259;Compliance">
        <rdfs:subClassOf rdf:resource="&iso5259;HybridCharacteristic"/>
        <rdfs:label xml:lang="en">Compliance</rdfs:label>
        <rdfs:label xml:lang="ko">준수성</rdfs:label>
        <rdfs:comment xml:lang="en">Degree to which data meet regulations and standards</rdfs:comment>
        <rdfs:comment xml:lang="ko">데이터가 규정 및 표준을 충족하는 정도</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="&iso5259;Efficiency">
        <rdfs:subClassOf rdf:resource="&iso5259;HybridCharacteristic"/>
        <rdfs:label xml:lang="en">Efficiency</rdfs:label>
        <rdfs:label xml:lang="ko">효율성</rdfs:label>
        <rdfs:comment xml:lang="en">Degree to which data can be processed with appropriate resources</rdfs:comment>
        <rdfs:comment xml:lang="ko">적절한 리소스로 데이터를 처리할 수 있는 정도</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="&iso5259;Precision">
        <rdfs:subClassOf rdf:resource="&iso5259;HybridCharacteristic"/>
        <rdfs:label xml:lang="en">Precision</rdfs:label>
        <rdfs:label xml:lang="ko">정밀성</rdfs:label>
        <rdfs:comment xml:lang="en">Degree to which data are exact or provide discrimination</rdfs:comment>
        <rdfs:comment xml:lang="ko">데이터가 정확하거나 구별력을 제공하는 정도</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="&iso5259;Traceability">
        <rdfs:subClassOf rdf:resource="&iso5259;HybridCharacteristic"/>
        <rdfs:label xml:lang="en">Traceability</rdfs:label>
        <rdfs:label xml:lang="ko">추적성</rdfs:label>
        <rdfs:comment xml:lang="en">Degree to which data provide an audit trail</rdfs:comment>
        <rdfs:comment xml:lang="ko">데이터가 감사 추적을 제공하는 정도</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="&iso5259;Understandability">
        <rdfs:subClassOf rdf:resource="&iso5259;HybridCharacteristic"/>
        <rdfs:label xml:lang="en">Understandability</rdfs:label>
        <rdfs:label xml:lang="ko">이해가능성</rdfs:label>
        <rdfs:comment xml:lang="en">Degree to which users can read and interpret data</rdfs:comment>
        <rdfs:comment xml:lang="ko">사용자가 데이터를 읽고 해석할 수 있는 정도</rdfs:comment>
    </owl:Class>

    <!-- System-Dependent Characteristics (Section 6.4) -->

    <owl:Class rdf:about="&iso5259;Availability">
        <rdfs:subClassOf rdf:resource="&iso5259;SystemDependentCharacteristic"/>
        <rdfs:label xml:lang="en">Availability</rdfs:label>
        <rdfs:label xml:lang="ko">가용성</rdfs:label>
        <rdfs:comment xml:lang="en">Degree to which datasets can be retrieved by authorized users</rdfs:comment>
        <rdfs:comment xml:lang="ko">권한 있는 사용자가 데이터셋을 검색할 수 있는 정도</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="&iso5259;Portability">
        <rdfs:subClassOf rdf:resource="&iso5259;SystemDependentCharacteristic"/>
        <rdfs:label xml:lang="en">Portability</rdfs:label>
        <rdfs:label xml:lang="ko">이식성</rdfs:label>
        <rdfs:comment xml:lang="en">Ability to move data from one system to another while preserving quality</rdfs:comment>
        <rdfs:comment xml:lang="ko">품질을 유지하면서 한 시스템에서 다른 시스템으로 데이터를 이동할 수 있는 능력</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="&iso5259;Recoverability">
        <rdfs:subClassOf rdf:resource="&iso5259;SystemDependentCharacteristic"/>
        <rdfs:label xml:lang="en">Recoverability</rdfs:label>
        <rdfs:label xml:lang="ko">복구가능성</rdfs:label>
        <rdfs:comment xml:lang="en">Degree to which datasets can be maintained and preserved</rdfs:comment>
        <rdfs:comment xml:lang="ko">데이터셋을 유지 및 보존할 수 있는 정도</rdfs:comment>
    </owl:Class>

    <!-- Additional Characteristics (Section 6.5) -->

    <owl:Class rdf:about="&iso5259;Auditability">
        <rdfs:subClassOf rdf:resource="&iso5259;DataQualityCharacteristic"/>
        <rdfs:label xml:lang="en">Auditability</rdfs:label>
        <rdfs:label xml:lang="ko">감사가능성</rdfs:label>
        <rdfs:comment xml:lang="en">Characteristic that dataset has undergone or is available for audit</rdfs:comment>
        <rdfs:comment xml:lang="ko">데이터셋이 감사를 거쳤거나 감사에 사용할 수 있는 특성</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="&iso5259;Balance">
        <rdfs:subClassOf rdf:resource="&iso5259;DataQualityCharacteristic"/>
        <rdfs:label xml:lang="en">Balance</rdfs:label>
        <rdfs:label xml:lang="ko">균형성</rdfs:label>
        <rdfs:comment xml:lang="en">Distribution of samples for all features of the dataset</rdfs:comment>
        <rdfs:comment xml:lang="ko">데이터셋의 모든 특성에 대한 샘플 분포</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="&iso5259;Diversity">
        <rdfs:subClassOf rdf:resource="&iso5259;DataQualityCharacteristic"/>
        <rdfs:label xml:lang="en">Diversity</rdfs:label>
        <rdfs:label xml:lang="ko">다양성</rdfs:label>
        <rdfs:comment xml:lang="en">Degree of difference between samples in the dataset</rdfs:comment>
        <rdfs:comment xml:lang="ko">데이터셋 내 샘플 간 차이 정도</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="&iso5259;Effectiveness">
        <rdfs:subClassOf rdf:resource="&iso5259;DataQualityCharacteristic"/>
        <rdfs:label xml:lang="en">Effectiveness</rdfs:label>
        <rdfs:label xml:lang="ko">효과성</rdfs:label>
        <rdfs:comment xml:lang="en">Degree to which dataset meets requirements for specific ML task</rdfs:comment>
        <rdfs:comment xml:lang="ko">데이터셋이 특정 ML 작업 요구사항을 충족하는 정도</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="&iso5259;Identifiability">
        <rdfs:subClassOf rdf:resource="&iso5259;DataQualityCharacteristic"/>
        <rdfs:label xml:lang="en">Identifiability</rdfs:label>
        <rdfs:label xml:lang="ko">식별가능성</rdfs:label>
        <rdfs:comment xml:lang="en">Capability to identify a PII principal from the dataset</rdfs:comment>
        <rdfs:comment xml:lang="ko">데이터셋에서 개인 식별 정보 주체를 식별할 수 있는 능력</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="&iso5259;Relevance">
        <rdfs:subClassOf rdf:resource="&iso5259;DataQualityCharacteristic"/>
        <rdfs:label xml:lang="en">Relevance</rdfs:label>
        <rdfs:label xml:lang="ko">관련성</rdfs:label>
        <rdfs:comment xml:lang="en">Degree to which dataset is suitable for given context</rdfs:comment>
        <rdfs:comment xml:lang="ko">데이터셋이 주어진 맥락에 적합한 정도</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="&iso5259;Representativeness">
        <rdfs:subClassOf rdf:resource="&iso5259;DataQualityCharacteristic"/>
        <rdfs:label xml:lang="en">Representativeness</rdfs:label>
        <rdfs:label xml:lang="ko">대표성</rdfs:label>
        <rdfs:comment xml:lang="en">Degree to which dataset reflects the target population</rdfs:comment>
        <rdfs:comment xml:lang="ko">데이터셋이 목표 모집단을 반영하는 정도</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="&iso5259;Similarity">
        <rdfs:subClassOf rdf:resource="&iso5259;DataQualityCharacteristic"/>
        <rdfs:label xml:lang="en">Similarity</rdfs:label>
        <rdfs:label xml:lang="ko">유사성</rdfs:label>
        <rdfs:comment xml:lang="en">Similarity between samples in terms of features</rdfs:comment>
        <rdfs:comment xml:lang="ko">특성 측면에서 샘플 간 유사성</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="&iso5259;Timeliness">
        <rdfs:subClassOf rdf:resource="&iso5259;DataQualityCharacteristic"/>
        <rdfs:label xml:lang="en">Timeliness</rdfs:label>
        <rdfs:label xml:lang="ko">적시성</rdfs:label>
        <rdfs:comment xml:lang="en">Latency between phenomenon occurrence and data availability</rdfs:comment>
        <rdfs:comment xml:lang="ko">현상 발생과 데이터 가용성 간의 지연 시간</rdfs:comment>
    </owl:Class>

    <!--
    =====================================================================
    Object Properties - 객체 속성 정의
    =====================================================================
    -->

    <owl:ObjectProperty rdf:about="&iso5259;hasCharacteristic">
        <rdfs:domain rdf:resource="&iso5259;DataQualityModel"/>
        <rdfs:range rdf:resource="&iso5259;DataQualityCharacteristic"/>
        <rdfs:label xml:lang="en">has characteristic</rdfs:label>
        <rdfs:label xml:lang="ko">특성을 가짐</rdfs:label>
        <rdfs:comment xml:lang="en">Relates a data quality model to its characteristics</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&iso5259;hasSubCharacteristic">
        <rdfs:domain rdf:resource="&iso5259;DataQualityCharacteristic"/>
        <rdfs:range rdf:resource="&iso5259;DataQualityCharacteristic"/>
        <rdfs:label xml:lang="en">has sub-characteristic</rdfs:label>
        <rdfs:label xml:lang="ko">하위 특성을 가짐</rdfs:label>
        <rdfs:comment xml:lang="en">Relates a characteristic to its sub-characteristics</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&iso5259;measuredBy">
        <rdfs:domain rdf:resource="&iso5259;DataQualityCharacteristic"/>
        <rdfs:range rdf:resource="&iso5259;DataQualityMeasure"/>
        <rdfs:label xml:lang="en">measured by</rdfs:label>
        <rdfs:label xml:lang="ko">~로 측정됨</rdfs:label>
        <rdfs:comment xml:lang="en">Relates a characteristic to its quality measures</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&iso5259;appliesTo">
        <rdfs:domain rdf:resource="&iso5259;DataQualityMeasure"/>
        <rdfs:range rdf:resource="&iso5259;TargetData"/>
        <rdfs:label xml:lang="en">applies to</rdfs:label>
        <rdfs:label xml:lang="ko">~에 적용됨</rdfs:label>
        <rdfs:comment xml:lang="en">Relates a measure to target data</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&iso5259;usesFunction">
        <rdfs:domain rdf:resource="&iso5259;DataQualityMeasure"/>
        <rdfs:range rdf:resource="&iso5259;MeasurementFunction"/>
        <rdfs:label xml:lang="en">uses function</rdfs:label>
        <rdfs:label xml:lang="ko">함수를 사용함</rdfs:label>
