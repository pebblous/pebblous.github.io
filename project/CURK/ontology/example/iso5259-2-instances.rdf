        <rdfs:label xml:lang="en">Referential Integrity Result - Customer Dataset</rdfs:label>
        <rdfs:label xml:lang="ko">참조 무결성 측정 결과 - 고객 데이터셋</rdfs:label>
        <iso5259:measuredWith rdf:resource="&iso5259;DQM_05"/>
        <iso5259:appliesTo rdf:resource="&iso5259;CustomerDataset"/>
        <iso5259:measurementResult rdf:datatype="&xsd;decimal">0.99</iso5259:measurementResult>
        <iso5259:measurementDate rdf:datatype="&xsd;date">2025-11-01</iso5259:measurementDate>
        <rdfs:comment xml:lang="en">99% of foreign key references are valid</rdfs:comment>
        <rdfs:comment xml:lang="ko">외래 키 참조의 99%가 유효함</rdfs:comment>
    </owl:NamedIndividual>

