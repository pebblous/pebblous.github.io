        <rdfs:comment xml:lang="en">97% of customer data is syntactically correct</rdfs:comment>
        <rdfs:comment xml:lang="ko">고객 데이터의 97%가 구문적으로 올바름</rdfs:comment>
    </owl:NamedIndividual>

    <!-- Measurement Result: DQM-02 on Customer Dataset -->
    <owl:NamedIndividual rdf:about="&iso5259;RESULT_DQM02_CUSTOMER">
        <rdf:type rdf:resource="&iso5259;MeasurementResult"/>
        <rdfs:label xml:lang="en">Semantic Accuracy Result - Customer Dataset</rdfs:label>
        <rdfs:label xml:lang="ko">의미적 정확성 측정 결과 - 고객 데이터셋</rdfs:label>
        <iso5259:measuredWith rdf:resource="&iso5259;DQM_02"/>
        <iso5259:appliesTo rdf:resource="&iso5259;CustomerDataset"/>
