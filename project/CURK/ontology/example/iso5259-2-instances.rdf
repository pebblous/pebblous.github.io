    <!--
    =====================================================================
    Additional Properties (defined here for instances)
    =====================================================================
    -->

    <!-- MeasurementResult class -->
    <owl:Class rdf:about="&iso5259;MeasurementResult">
        <rdfs:label xml:lang="en">Measurement Result</rdfs:label>
        <rdfs:label xml:lang="ko">측정 결과</rdfs:label>
        <rdfs:comment xml:lang="en">The result of applying a quality measure to target data</rdfs:comment>
        <rdfs:comment xml:lang="ko">대상 데이터에 품질 측정을 적용한 결과</rdfs:comment>
    </owl:Class>

