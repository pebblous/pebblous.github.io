<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#">
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
    <!ENTITY iso5259 "http://pebblous.ai/ontology/iso5259-2#">
]>

<rdf:RDF xmlns="http://pebblous.ai/ontology/iso5259-2#"
     xml:base="http://pebblous.ai/ontology/iso5259-2"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:iso5259="http://pebblous.ai/ontology/iso5259-2#">

    <!--
    =====================================================================
    ISO/IEC 5259-2 Data Quality Measures - Instances
    실제 측정 항목(Individuals) 및 예제 데이터
    =====================================================================
    -->

    <owl:Ontology rdf:about="http://pebblous.ai/ontology/iso5259-2/instances">
        <rdfs:label xml:lang="en">ISO/IEC 5259-2 Data Quality Measures - Instances</rdfs:label>
        <rdfs:label xml:lang="ko">ISO/IEC 5259-2 데이터 품질 측정 지표 - 인스턴스</rdfs:label>
        <owl:imports rdf:resource="http://pebblous.ai/ontology/iso5259-2"/>
        <owl:versionInfo>1.0.0</owl:versionInfo>
    </owl:Ontology>

    <!--
    =====================================================================
    Measurement Functions - 측정 함수들
    =====================================================================
    -->

    <!-- MF-01: Ratio Calculation -->
    <owl:NamedIndividual rdf:about="&iso5259;MF_01">
        <rdf:type rdf:resource="&iso5259;MeasurementFunction"/>
        <rdfs:label xml:lang="en">Ratio Measurement Function</rdfs:label>
        <rdfs:label xml:lang="ko">비율 측정 함수</rdfs:label>
        <iso5259:measurementFormula>X / Y</iso5259:measurementFormula>
        <rdfs:comment xml:lang="en">General ratio calculation: X (numerator) divided by Y (denominator)</rdfs:comment>
        <rdfs:comment xml:lang="ko">일반 비율 계산: X (분자)를 Y (분모)로 나눔</rdfs:comment>
    </owl:NamedIndividual>

    <!-- MF-02: Percentage Calculation -->
    <owl:NamedIndividual rdf:about="&iso5259;MF_02">
        <rdf:type rdf:resource="&iso5259;MeasurementFunction"/>
        <rdfs:label xml:lang="en">Percentage Measurement Function</rdfs:label>
        <rdfs:label xml:lang="ko">백분율 측정 함수</rdfs:label>
        <iso5259:measurementFormula>(X / Y) * 100</iso5259:measurementFormula>
        <rdfs:comment xml:lang="en">Percentage calculation: (X / Y) * 100</rdfs:comment>
        <rdfs:comment xml:lang="ko">백분율 계산: (X / Y) * 100</rdfs:comment>
    </owl:NamedIndividual>

    <!-- MF-03: Count Function -->
    <owl:NamedIndividual rdf:about="&iso5259;MF_03">
        <rdf:type rdf:resource="&iso5259;MeasurementFunction"/>
        <rdfs:label xml:lang="en">Count Function</rdfs:label>
        <rdfs:label xml:lang="ko">개수 함수</rdfs:label>
        <iso5259:measurementFormula>COUNT(X WHERE condition)</iso5259:measurementFormula>
        <rdfs:comment xml:lang="en">Count elements satisfying a condition</rdfs:comment>
        <rdfs:comment xml:lang="ko">조건을 만족하는 요소의 개수</rdfs:comment>
    </owl:NamedIndividual>

    <!--
    =====================================================================
    Data Quality Measures - Accuracy (정확성)
    =====================================================================
    -->

    <!-- DQM-01: Syntactic Accuracy -->
    <owl:NamedIndividual rdf:about="&iso5259;DQM_01">
        <rdf:type rdf:resource="&iso5259;DataQualityMeasure"/>
        <rdfs:label xml:lang="en">Syntactic Accuracy</rdfs:label>
        <rdfs:label xml:lang="ko">구문적 정확성</rdfs:label>
        <iso5259:measureID>DQM-01</iso5259:measureID>
        <iso5259:measureName xml:lang="en">Syntactic accuracy</iso5259:measureName>
        <iso5259:measureName xml:lang="ko">구문적 정확성</iso5259:measureName>
        <iso5259:measureDescription xml:lang="en">Ratio of data items that are syntactically correct to total number of data items</iso5259:measureDescription>
        <iso5259:measureDescription xml:lang="ko">구문적으로 올바른 데이터 항목의 비율</iso5259:measureDescription>
        <iso5259:usesFunction rdf:resource="&iso5259;MF_01"/>
        <iso5259:standardReference>ISO/IEC 5259-2:2024, Table 1</iso5259:standardReference>
    </owl:NamedIndividual>

    <!-- DQM-02: Semantic Accuracy -->
    <owl:NamedIndividual rdf:about="&iso5259;DQM_02">
        <rdf:type rdf:resource="&iso5259;DataQualityMeasure"/>
        <rdfs:label xml:lang="en">Semantic Accuracy</rdfs:label>
        <rdfs:label xml:lang="ko">의미적 정확성</rdfs:label>
        <iso5259:measureID>DQM-02</iso5259:measureID>
        <iso5259:measureName xml:lang="en">Semantic accuracy</iso5259:measureName>
        <iso5259:measureName xml:lang="ko">의미적 정확성</iso5259:measureName>
        <iso5259:measureDescription xml:lang="en">Ratio of data items that are semantically correct to total number of data items</iso5259:measureDescription>
        <iso5259:measureDescription xml:lang="ko">의미적으로 올바른 데이터 항목의 비율</iso5259:measureDescription>
        <iso5259:usesFunction rdf:resource="&iso5259;MF_01"/>
        <iso5259:standardReference>ISO/IEC 5259-2:2024, Table 1</iso5259:standardReference>
    </owl:NamedIndividual>

    <!--
    =====================================================================
    Data Quality Measures - Completeness (완전성)
    =====================================================================
    -->

    <!-- DQM-03: Data Completeness -->
    <owl:NamedIndividual rdf:about="&iso5259;DQM_03">
        <rdf:type rdf:resource="&iso5259;DataQualityMeasure"/>
        <rdfs:label xml:lang="en">Data Completeness</rdfs:label>
        <rdfs:label xml:lang="ko">데이터 완전성</rdfs:label>
        <iso5259:measureID>DQM-03</iso5259:measureID>
        <iso5259:measureName xml:lang="en">Data completeness</iso5259:measureName>
        <iso5259:measureName xml:lang="ko">데이터 완전성</iso5259:measureName>
        <iso5259:measureDescription xml:lang="en">Ratio of non-null data items to total number of data items</iso5259:measureDescription>
        <iso5259:measureDescription xml:lang="ko">NULL이 아닌 데이터 항목의 비율</iso5259:measureDescription>
        <iso5259:usesFunction rdf:resource="&iso5259;MF_01"/>
        <iso5259:standardReference>ISO/IEC 5259-2:2024, Table 2</iso5259:standardReference>
    </owl:NamedIndividual>

    <!-- DQM-04: Schema Completeness -->
    <owl:NamedIndividual rdf:about="&iso5259;DQM_04">
        <rdf:type rdf:resource="&iso5259;DataQualityMeasure"/>
        <rdfs:label xml:lang="en">Schema Completeness</rdfs:label>
        <rdfs:label xml:lang="ko">스키마 완전성</rdfs:label>
        <iso5259:measureID>DQM-04</iso5259:measureID>
        <iso5259:measureName xml:lang="en">Schema completeness</iso5259:measureName>
        <iso5259:measureName xml:lang="ko">스키마 완전성</iso5259:measureName>
        <iso5259:measureDescription xml:lang="en">Ratio of data items defined in schema that are populated</iso5259:measureDescription>
        <iso5259:measureDescription xml:lang="ko">스키마에 정의된 데이터 항목 중 채워진 비율</iso5259:measureDescription>
        <iso5259:usesFunction rdf:resource="&iso5259;MF_01"/>
        <iso5259:standardReference>ISO/IEC 5259-2:2024, Table 2</iso5259:standardReference>
    </owl:NamedIndividual>

    <!--
    =====================================================================
    Data Quality Measures - Consistency (일관성)
    =====================================================================
    -->

    <!-- DQM-05: Referential Integrity -->
    <owl:NamedIndividual rdf:about="&iso5259;DQM_05">
        <rdf:type rdf:resource="&iso5259;DataQualityMeasure"/>
        <rdfs:label xml:lang="en">Referential Integrity</rdfs:label>
        <rdfs:label xml:lang="ko">참조 무결성</rdfs:label>
        <iso5259:measureID>DQM-05</iso5259:measureID>
        <iso5259:measureName xml:lang="en">Referential integrity</iso5259:measureName>
        <iso5259:measureName xml:lang="ko">참조 무결성</iso5259:measureName>
        <iso5259:measureDescription xml:lang="en">Ratio of valid foreign key references to total foreign keys</iso5259:measureDescription>
        <iso5259:measureDescription xml:lang="ko">유효한 외래 키 참조의 비율</iso5259:measureDescription>
        <iso5259:usesFunction rdf:resource="&iso5259;MF_01"/>
        <iso5259:standardReference>ISO/IEC 5259-2:2024, Table 3</iso5259:standardReference>
    </owl:NamedIndividual>

    <!-- DQM-06: Format Consistency -->
    <owl:NamedIndividual rdf:about="&iso5259;DQM_06">
        <rdf:type rdf:resource="&iso5259;DataQualityMeasure"/>
        <rdfs:label xml:lang="en">Format Consistency</rdfs:label>
        <rdfs:label xml:lang="ko">형식 일관성</rdfs:label>
        <iso5259:measureID>DQM-06</iso5259:measureID>
        <iso5259:measureName xml:lang="en">Format consistency</iso5259:measureName>
        <iso5259:measureName xml:lang="ko">형식 일관성</iso5259:measureName>
        <iso5259:measureDescription xml:lang="en">Ratio of data items conforming to specified format</iso5259:measureDescription>
        <iso5259:measureDescription xml:lang="ko">지정된 형식을 따르는 데이터 항목의 비율</iso5259:measureDescription>
        <iso5259:usesFunction rdf:resource="&iso5259;MF_01"/>
        <iso5259:standardReference>ISO/IEC 5259-2:2024, Table 3</iso5259:standardReference>
    </owl:NamedIndividual>

    <!--
    =====================================================================
    Relationships: Characteristic → Measure
    =====================================================================
    -->

    <!-- Accuracy measured by DQM-01, DQM-02 -->
    <owl:NamedIndividual rdf:about="&iso5259;Accuracy">
        <iso5259:measuredBy rdf:resource="&iso5259;DQM_01"/>
        <iso5259:measuredBy rdf:resource="&iso5259;DQM_02"/>
    </owl:NamedIndividual>

    <!-- Completeness measured by DQM-03, DQM-04 -->
    <owl:NamedIndividual rdf:about="&iso5259;Completeness">
        <iso5259:measuredBy rdf:resource="&iso5259;DQM_03"/>
        <iso5259:measuredBy rdf:resource="&iso5259;DQM_04"/>
    </owl:NamedIndividual>

    <!-- Consistency measured by DQM-05, DQM-06 -->
    <owl:NamedIndividual rdf:about="&iso5259;Consistency">
        <iso5259:measuredBy rdf:resource="&iso5259;DQM_05"/>
        <iso5259:measuredBy rdf:resource="&iso5259;DQM_06"/>
    </owl:NamedIndividual>

    <!--
    =====================================================================
    Example Dataset - 예제 데이터셋
    =====================================================================
    -->

    <!-- Example: Customer Dataset -->
    <owl:NamedIndividual rdf:about="&iso5259;CustomerDataset">
        <rdf:type rdf:resource="&iso5259;DataSet"/>
        <rdfs:label xml:lang="en">Customer Dataset</rdfs:label>
        <rdfs:label xml:lang="ko">고객 데이터셋</rdfs:label>
        <rdfs:comment xml:lang="en">Sample customer data for ML model training</rdfs:comment>
        <rdfs:comment xml:lang="ko">ML 모델 학습용 고객 데이터 샘플</rdfs:comment>
        <iso5259:datasetSize rdf:datatype="&xsd;integer">10000</iso5259:datasetSize>
        <iso5259:numberOfAttributes rdf:datatype="&xsd;integer">15</iso5259:numberOfAttributes>
    </owl:NamedIndividual>

    <!--
    =====================================================================
    Example Quality Requirements - 예제 품질 요구사항
    =====================================================================
    -->

    <!-- Requirement: Accuracy >= 0.95 -->
    <owl:NamedIndividual rdf:about="&iso5259;REQ_ACC_001">
        <rdf:type rdf:resource="&iso5259;DataQualityRequirement"/>
        <rdfs:label xml:lang="en">Accuracy Requirement - Customer Dataset</rdfs:label>
        <rdfs:label xml:lang="ko">정확성 요구사항 - 고객 데이터셋</rdfs:label>
        <iso5259:hasCharacteristic rdf:resource="&iso5259;Accuracy"/>
        <iso5259:appliesTo rdf:resource="&iso5259;CustomerDataset"/>
        <iso5259:threshold rdf:datatype="&xsd;decimal">0.95</iso5259:threshold>
        <rdfs:comment xml:lang="en">Customer data must have at least 95% accuracy</rdfs:comment>
        <rdfs:comment xml:lang="ko">고객 데이터는 최소 95% 정확성을 가져야 함</rdfs:comment>
    </owl:NamedIndividual>

    <!-- Requirement: Completeness >= 0.90 -->
    <owl:NamedIndividual rdf:about="&iso5259;REQ_COM_001">
        <rdf:type rdf:resource="&iso5259;DataQualityRequirement"/>
        <rdfs:label xml:lang="en">Completeness Requirement - Customer Dataset</rdfs:label>
        <rdfs:label xml:lang="ko">완전성 요구사항 - 고객 데이터셋</rdfs:label>
        <iso5259:hasCharacteristic rdf:resource="&iso5259;Completeness"/>
        <iso5259:appliesTo rdf:resource="&iso5259;CustomerDataset"/>
        <iso5259:threshold rdf:datatype="&xsd;decimal">0.90</iso5259:threshold>
        <rdfs:comment xml:lang="en">Customer data must be at least 90% complete</rdfs:comment>
        <rdfs:comment xml:lang="ko">고객 데이터는 최소 90% 완전해야 함</rdfs:comment>
    </owl:NamedIndividual>

    <!-- Requirement: Consistency >= 0.98 -->
    <owl:NamedIndividual rdf:about="&iso5259;REQ_CON_001">
        <rdf:type rdf:resource="&iso5259;DataQualityRequirement"/>
        <rdfs:label xml:lang="en">Consistency Requirement - Customer Dataset</rdfs:label>
        <rdfs:label xml:lang="ko">일관성 요구사항 - 고객 데이터셋</rdfs:label>
        <iso5259:hasCharacteristic rdf:resource="&iso5259;Consistency"/>
        <iso5259:appliesTo rdf:resource="&iso5259;CustomerDataset"/>
        <iso5259:threshold rdf:datatype="&xsd;decimal">0.98</iso5259:threshold>
        <rdfs:comment xml:lang="en">Customer data must be at least 98% consistent</rdfs:comment>
        <rdfs:comment xml:lang="ko">고객 데이터는 최소 98% 일관성을 가져야 함</rdfs:comment>
    </owl:NamedIndividual>

    <!--
    =====================================================================
    Example Measurement Results - 예제 측정 결과
    =====================================================================
    -->

